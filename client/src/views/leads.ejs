<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Leads</title>
</head>
<body>
    <h1>Leads</h1>
    <ul>
        <% leads.forEach(function(lead) { %>
            <li>
                <strong><%= lead.name %></strong> - <%= lead.company %> - <%= lead.status %>
            </li>
        <% }); %>
    </ul>
</body>
</html>
